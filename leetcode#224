class Solution {
    public boolean checkIfExist(int[] arr) {
        for (int i = 0; i <= arr.length - 1; i++){
            int j = i + 1;
            while (j < arr.length){
                if (arr[i] == 2*arr[j] || arr[j] == 2*arr[i]){
                    return true;
                }
                else{
                    j++;
                   
                }
                
            }
            
        }
        return false;
        
    }
}
